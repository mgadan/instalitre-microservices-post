curl -X POST \                   
    -H "Content-Type: application/x-www-form-urlencoded" \   
    -d 'username=test&password=test44&grant_type=password' \
    -d 'client_id=web' \
    -d 'client_secret=8fe2be27-821f-4d50-a9a3-2ca6c2f1327d' \
    "http://localhost:8080/auth/realms/instalitre/protocol/openid-connect/token"


access_token=$(curl \
-d "client_id=web" \
-d client_secret=8fe2be27-821f-4d50-a9a3-2ca6c2f1327d \
-d "username=test" \
-d "password=test44!" \
-d "grant_type=password" \
"http://localhost:8080/auth/realms/instalitre/protocol/openid-connect/token" | jq -r
'.access_token')


access_token=curl \
-d "client_id=api" -d "client_secret=8fe2be27-821f-4d50-a9a3-2ca6c2f1327d" \
-d "username=test" -d "password=test44" \
-d "grant_type=password" \
-d "scope=bearer" \
http://localhost:8080/auth/realms/instalitre/protocol/openid-connect/token | jq -r '.access_token'
curl –header « Authorization: Bearer $access_token » \
http://localhost:8080/auth/realms/instalitre/protocol/openid-connect/userinfo | jq .

